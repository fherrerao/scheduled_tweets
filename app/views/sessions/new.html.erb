<%= form_with url: sign_in_path, data: { turbo: false } do |form| %>
  <div class="mb-3">
    <%= form.label :email %>
    <%= form.email_field :email, class: "form-control", placeholder: "test@test.com" %>
  </div>
  <div class="mb-3">
    <%= form.label :password %>
    <%= form.password_field :password, class: "form-control", placeholder: "123456" %>
    <%= link_to "Forgot your password?", password_reset_path %>
  </div>
  <%= form.submit "Log in", class: "btn btn-primary" %>
<%end%>
